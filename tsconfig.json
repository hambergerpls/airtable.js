        "lib": [
            "ESNext"
        ],
        "target": "ESNext",
        "module": "ESNext",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "lib",
        "noImplicitAny": false,
        "strict": false,
        "sourceMap": true,
        "declaration": true
    },
    "include": [
